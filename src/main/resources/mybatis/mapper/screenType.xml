<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sType">
    <select id="getPrice" resultType="mybatis.vo.ScreenTypeVO">
        select * from screentype
    </select>

    <select id="getType" resultType="mybatis.vo.ScreenTypeVO" parameterType="mybatis.vo.ScreenTypeVO">
        select * from screentype
        WHERE screenCode = #{screenCode}
    </select>
</mapper>